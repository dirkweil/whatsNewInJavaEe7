<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <navigation-rule>
    <navigation-case>
      <from-outcome>home</from-outcome>
      <to-view-id>/index</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>flowB1-return</from-outcome>
      <to-view-id>/index</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>flowB2-return</from-outcome>
      <to-view-id>/index</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>flowC-return</from-outcome>
      <to-view-id>/index</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>
